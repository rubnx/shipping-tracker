{
  "name": "shipping-tracker-api",
  "version": "1.0.0",
  "description": "Backend API for shipping tracking application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:comprehensive": "ts-node src/scripts/runComprehensiveTests.ts",
    "test:all-apis": "jest src/test/AllAPIsIntegration.test.ts --testTimeout=60000",
    "test:performance": "jest src/test/PerformanceMonitoring.test.ts --testTimeout=300000",
    "test:full-suite": "ts-node src/scripts/runAllTests.ts",
    "test:repositories": "ts-node src/test/repositories.test.ts",
    "test:db": "node test-database.js",
    "monitor:apis": "ts-node src/scripts/monitorAPIs.ts",
    "monitor:report": "ts-node src/scripts/monitorAPIs.ts --report",
    "migrate": "ts-node src/scripts/migrate.ts",
    "setup:env": "ts-node src/scripts/setupEnvironment.ts",
    "setup:db": "ts-node src/scripts/setupDatabase.ts",
    "setup:db:reset": "ts-node src/scripts/setupDatabase.ts --reset",
    "validate:env": "ts-node src/scripts/validateEnvironment.ts",
    "validate:keys": "ts-node src/scripts/validateEnvironment.ts"
  },
  "keywords": [
    "shipping",
    "tracking",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.23",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
