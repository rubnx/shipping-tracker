#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_a94835b39154933198bbdf9f1f9258e0/node_modules/ts-jest/node_modules:/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_a94835b39154933198bbdf9f1f9258e0/node_modules:/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_a94835b39154933198bbdf9f1f9258e0/node_modules/ts-jest/node_modules:/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_a94835b39154933198bbdf9f1f9258e0/node_modules:/Users/rubenruiz/Documents/Coding/Projects/shipping-tracker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
