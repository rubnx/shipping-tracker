# Staging Environment Configuration

# API Configuration
STAGING_API_URL=https://api-staging.shipping-tracker.com
STAGING_FRONTEND_URL=https://staging.shipping-tracker.com

# Database Configuration
DATABASE_URL=postgresql://staging_user:staging_pass@staging-db.com:5432/shipping_tracker_staging
REDIS_URL=redis://staging-redis.com:6379

# External APIs (use test/sandbox endpoints)
MAERSK_API_KEY=staging_maersk_key
MSC_API_KEY=staging_msc_key
PROJECT44_API_KEY=staging_project44_key

# Monitoring and Logging
SENTRY_DSN_FRONTEND=https://staging-frontend-dsn@sentry.io/project
SENTRY_DSN_BACKEND=https://staging-backend-dsn@sentry.io/project
LOG_LEVEL=debug

# Deployment Configuration
VERCEL_TOKEN=staging_vercel_token
NETLIFY_AUTH_TOKEN=staging_netlify_token
RAILWAY_TOKEN=staging_railway_token
RENDER_DEPLOY_HOOK_STAGING=https://api.render.com/deploy/staging-hook

# Security
JWT_SECRET=staging_jwt_secret_32_chars_minimum
API_SECRET_KEY=staging_api_secret_32_chars_minimum

# Feature Flags
DEMO_MODE=true
ENABLE_MOCK_DATA=true
RATE_LIMIT_MAX_REQUESTS=200